{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": [
    "plugin:vue/essential",
    "airbnb-base"
  ],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": [
    "vue",
    "graphql"
  ],
  "rules": {
    "import/no-unresolved": "off",
    "max-len": "off",
    "no-mixed-operators": "off",
    "no-underscore-dangle": "off",
    "no-return-assign": "off"
  }
}